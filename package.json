{
  "name": "@kasymjaparov/adihelper",
  "version": "0.0.8",
  "description": "Utility helpers",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "build:dev": "tsup --watch",
    "build:analyze": "tsup --metafile",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "utils",
    "typescript",
    "date",
    "string",
    "file",
    "helper"
  ],
  "author": "Kasymjaparov Adilet",
  "repository": {
    "type": "git",
    "url": "https://github.com/kasymjaparov/adihelper.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.0.10",
    "tsup": "^8.0.0",
    "typescript": "^5.3.3",
    "vitest": "^1.5.0"
  }
}
